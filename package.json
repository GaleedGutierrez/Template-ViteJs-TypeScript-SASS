{
	"name": "template-vitejs-typescript-sass",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"prepare": "husky install",
		"lint": "eslint ./",
		"lint:fix": "npx eslint --fix ./",
		"stylelint": "npx stylelint \"**/*.scss\"",
		"stylelint:fix": "npx stylelint --fix \"**/*.scss\"",
		"prettier": "prettier --write",
		"prettier:fix": "prettier --write *.html"
	},
	"description": "Template",
	"main": "index.js",
	"keywords": [
		"TypeScript",
		"JavaScript",
		"SASS",
		"CSS",
		"HTML"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/GaleedGutierrez/Template-ViteJs-TypeScript-SASS.git"
	},
	"author": "Galeed Gutierrez <galeedeveloper@gmail.com> (https://galeedgutierrez.com/)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/GaleedGutierrez/Template-ViteJs-TypeScript-SASS/issues"
	},
	"homepage": "https://github.com/GaleedGutierrez/Template-ViteJs-TypeScript-SASS#readme",
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^5.59.1",
		"@typescript-eslint/parser": "^5.59.1",
		"eslint": "^8.39.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-check-file": "^2.2.0",
		"eslint-plugin-editorconfig": "^4.0.2",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-simple-import-sort": "^10.0.0",
		"eslint-plugin-unused-imports": "^2.0.0",
		"husky": "^8.0.0",
		"lint-staged": "^13.2.2",
		"markdownlint": "0.28.2",
		"prettier": "^2.8.8",
		"sass": "^1.62.1",
		"stylelint": "^15.6.0",
		"stylelint-config-standard-scss": "^9.0.0",
		"stylelint-order": "^6.0.3",
		"stylelint-prettier": "^3.0.0",
		"typescript": "^5.0.2",
		"vite": "^4.3.2"
	},
	"lint-staged": {
		"*.(j|t)s": "eslint --fix",
		"*.s(a|c)ss": "stylelint --fix",
		"*.html": "prettier --write"
	}
}
